From 21c1c6c77b40ca030e6d72945e5b71c97f10b9f4 Mon Sep 17 00:00:00 2001
From: Joey Riches <josephriches@gmail.com>
Date: Mon, 18 Oct 2021 17:44:53 +0100
Subject: [PATCH 1/1] newer-keyring

---
 PKG-INFO       | 2 +-
 pyproject.toml | 2 +-
 setup.py       | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/PKG-INFO b/PKG-INFO
index 45d2cdb..9394c1d 100644
--- a/PKG-INFO
+++ b/PKG-INFO
@@ -32,7 +32,7 @@ Requires-Dist: html5lib (>=1.0,<2.0)
 Requires-Dist: importlib-metadata (>=1.6.0,<2.0.0); python_version < "3.8"
 Requires-Dist: keyring (>=18.0.1,<19.0.0); python_version >= "2.7" and python_version < "2.8"
 Requires-Dist: keyring (>=20.0.1,<21.0.0); python_version >= "3.5" and python_version < "3.6"
-Requires-Dist: keyring (>=21.2.0,<22.0.0); python_version >= "3.6" and python_version < "4.0"
+Requires-Dist: keyring (>=21.2.0,<23.0.0); python_version >= "3.6" and python_version < "4.0"
 Requires-Dist: packaging (>=20.4,<21.0)
 Requires-Dist: pathlib2 (>=2.3,<3.0); python_version >= "2.7" and python_version < "2.8"
 Requires-Dist: pexpect (>=4.7.0,<5.0.0)
diff --git a/pyproject.toml b/pyproject.toml
index 1f3abae..800ee51 100644
--- a/pyproject.toml
+++ b/pyproject.toml
@@ -54,7 +54,7 @@ functools32 = { version = "^3.2.3", python = "~2.7" }
 keyring = [
     { version = "^18.0.1", python = "~2.7" },
     { version = "^20.0.1", python = "~3.5" },
-    { version = "^21.2.0", python = "^3.6" }
+    { version = "^23.0.0", python = "^3.6" }
 ]
 # Use subprocess32 for Python 2.7
 subprocess32 = { version = "^3.5", python = "~2.7" }
diff --git a/setup.py b/setup.py
index b16dbdc..39e9d64 100644
--- a/setup.py
+++ b/setup.py
@@ -63,7 +63,7 @@ extras_require = \
                                                          'subprocess32>=3.5,<4.0'],
  ':python_version >= "3.5" and python_version < "3.6"': ['keyring>=20.0.1,<21.0.0'],
  ':python_version >= "3.6" and python_version < "4.0"': ['crashtest>=0.3.0,<0.4.0',
-                                                         'keyring>=21.2.0,<22.0.0']}
+                                                         'keyring>=21.2.0,<23.0.0']}
 
 entry_points = \
 {'console_scripts': ['poetry = poetry.console:main']}
-- 
